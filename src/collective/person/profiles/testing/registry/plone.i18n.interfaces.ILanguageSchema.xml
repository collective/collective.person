<?xml version="1.0" encoding="utf-8"?>
<registry>
  <records interface="plone.i18n.interfaces.ILanguageSchema"
           prefix="plone"
           purge="true"
  >
    <value key="available_languages">
      <element>en</element>
      <element>de</element>
      <element>es</element>
      <element>fr</element>
      <element>pt-br</element>
      <element>nl</element>
    </value>
    <value key="use_combined_language_codes">True</value>
    <value key="default_language">en</value>
  </records>
</registry>
